MODEL CatalogueObjects_V2 (en)
  AT "https://models.geo.admin.ch/CH/" VERSION "2021-01-06" =

  TOPIC Catalogues (ABSTRACT) =

    CLASS Item (ABSTRACT) =
    END Item;

  END Catalogues;

