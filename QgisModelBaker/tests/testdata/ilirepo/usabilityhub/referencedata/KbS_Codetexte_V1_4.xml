      </KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition>
      <KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition TID="Legende_StatusAltlV6">
        <Code>StatusAltlV6</Code>
        <Definition>
          <LocalisationCH_V1.MultilingualText>
            <LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>de</Language>
                <Text>Belastet, Untersuchungsbedürftigkeit noch nicht definiert</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>fr</Language>
                <Text>Pollué, nécessité d'une investigation non encore évaluée</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>it</Language>
                <Text>Inquinato, la necessità di un'indagine non è ancora stata stabilita</Text>
              </LocalisationCH_V1.LocalisedText>
            </LocalisedText>
          </LocalisationCH_V1.MultilingualText>
        </Definition>
        <Symbol>
          <BINBLBOX></BINBLBOX>
        </Symbol>
      </KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition>

