      <KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition TID="Legende_StatusAltlV1">
        <Code>StatusAltlV1</Code>
        <Definition>
          <LocalisationCH_V1.MultilingualText>
            <LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>de</Language>
                <Text>Belastet, keine schädlichen oder lästigen Einwirkungen zu erwarten</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>fr</Language>
                <Text>Pollué, pas d'atteinte nuisible ou incommodante à attendre</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>it</Language>
                <Text>Inquinato, non sono prevedibili effetti dannosi o molesti</Text>
              </LocalisationCH_V1.LocalisedText>
            </LocalisedText>
          </LocalisationCH_V1.MultilingualText>
        </Definition>
        <Symbol>
          <BINBLBOX></BINBLBOX>
        </Symbol>
      </KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition>
      <KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition TID="Legende_StatusAltlV2">
        <Code>StatusAltlV2</Code>
        <Definition>
          <LocalisationCH_V1.MultilingualText>
            <LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>de</Language>
                <Text>Belastet, untersuchungsbedürftig</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>fr</Language>
                <Text>Pollué, investigation nécessaire</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>it</Language>
                <Text>Inquinato, è necessario procedere a un'indagine</Text>
              </LocalisationCH_V1.LocalisedText>
            </LocalisedText>
          </LocalisationCH_V1.MultilingualText>
        </Definition>
        <Symbol>
          <BINBLBOX></BINBLBOX>
        </Symbol>
      </KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition>
      <KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition TID="Legende_StatusAltlV3">
        <Code>StatusAltlV3</Code>
        <Definition>
          <LocalisationCH_V1.MultilingualText>
            <LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>de</Language>
                <Text>Belastet, weder überwachungs- noch sanierungsbedürftig</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>fr</Language>
                <Text>Pollué, ne nécessite ni surveillance ni assainissement</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>it</Language>
                <Text>Inquinato, non deve essere né sorvegliato né risanato</Text>
              </LocalisationCH_V1.LocalisedText>
            </LocalisedText>
          </LocalisationCH_V1.MultilingualText>
        </Definition>
        <Symbol>
          <BINBLBOX></BINBLBOX>
        </Symbol>
      </KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition>
      <KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition TID="Legende_StatusAltlV4">
        <Code>StatusAltlV4</Code>
        <Definition>
          <LocalisationCH_V1.MultilingualText>
            <LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>de</Language>
                <Text>Belastet, überwachungsbedürftig</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>fr</Language>
                <Text>Pollué, nécessite une surveillance</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>it</Language>
                <Text>Inquinato, deve essere sorvegliato</Text>
              </LocalisationCH_V1.LocalisedText>
            </LocalisedText>
          </LocalisationCH_V1.MultilingualText>
        </Definition>
        <Symbol>
          <BINBLBOX></BINBLBOX>
        </Symbol>
      </KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition>
      <KbS_Basis_V1_4.Codelisten.StatusAltlV_Definition TID="Legende_StatusAltlV5">
        <Code>StatusAltlV5</Code>
        <Definition>
          <LocalisationCH_V1.MultilingualText>
            <LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>de</Language>
                <Text>Belastet, sanierungsbedürftig</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>fr</Language>
                <Text>Pollué, nécessite un assainissement</Text>
              </LocalisationCH_V1.LocalisedText>
              <LocalisationCH_V1.LocalisedText>
                <Language>it</Language>
                <Text>Inquinato, deve essere risanato</Text>
              </LocalisationCH_V1.LocalisedText>
            </LocalisedText>
          </LocalisationCH_V1.MultilingualText>
        </Definition>
        <Symbol>
          <BINBLBOX></BINBLBOX>
